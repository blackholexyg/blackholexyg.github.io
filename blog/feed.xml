---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Yeguang Blog</title>
 <link href="http://blackholexyg.github.io/blog/feed.xml" rel="self" />
 <link href="http://blackholexyg.github.io/blog/" />
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blackholexyg.github.io/blog/</id>
 <author>
   <name>Yeguang Xue</name>
   <email>blackholexyg@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>
     {% if post.title-display %}
       {{ post.title-display }}
     {% else %}
       {{ post.title }}
     {% endif %}
   </title>
   <link href="http://blackholexyg.github.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://blackholexyg.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
